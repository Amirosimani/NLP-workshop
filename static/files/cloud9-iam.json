{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "FullAccess",
            "Effect": "Allow",
            "Action": [
                "s3:*", 
                "sagemaker:*", 
                "eks:*", 
                "ecr:*", 
                "ecs:*", 
                "ec2:*", 
                "lambda:*", 
                "ssm:*", 
                "cloudwatch:*", 
                "elasticloadbalancing:*",
                "autoscaling:*",
                "application-autoscaling:*",
                "iam:*",
                "events:*",
                "logs:*",
                "cloudformation:*",
                "route53:*"
            ],
            "Resource": ["*"]
        }
    ]
}
